<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_pascal_app_nav"
    app:startDestination="@id/messageFragment2">
    <fragment
        android:id="@+id/messageFragment2"
        android:name="com.example.enigmacampapp.screen.MessageFragment"
        android:label="fragment_message"
        tools:layout="@layout/fragment_message" >
        <action
            android:id="@+id/action_messageFragment2_to_homeFragment"
            app:destination="@id/homeFragment" />
        <argument
            android:name="passedMessageMF"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.enigmacampapp.screen.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <argument
            android:name="passedMessageHF"
            app:argType="string" />
    </fragment>
</navigation>